{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-dewest"
		},
		"ls_dest_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dest_adls'"
		},
		"ls_source_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_blob'"
		},
		"ls_sql_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_server'"
		},
		"ls_dest_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dladforder0001.dfs.core.windows.net/"
		},
		"ls_sql_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserver-order.database.windows.net"
		},
		"ls_sql_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqlserver-order"
		},
		"ls_sql_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "User"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/fe9e270c-5036-4a39-9aaa-6444d5cb7072/resourceGroups/rg-for-adf/providers/Microsoft.Storage/storageAccounts/sgorders001"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dladforder0001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-dewest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dest_adls_accountKey": {
						"value": "[parameters('ls_dest_adls_accountKey')]"
					},
					"ls_source_blob_connectionString": {
						"value": "[parameters('ls_source_blob_connectionString')]"
					},
					"ls_sql_server_password": {
						"value": "[parameters('ls_sql_server_password')]"
					},
					"ls_dest_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_dest_adls_properties_typeProperties_url')]"
					},
					"ls_sql_server_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_server')]"
					},
					"ls_sql_server_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_database')]"
					},
					"ls_sql_server_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_server_properties_typeProperties_userName')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-dev-dewest_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dev-dewest_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-dev-dewest_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-dev-dewest_ArmTemplate_1')]"
			]
		}
	]
}